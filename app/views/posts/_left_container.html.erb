<h1>Listing Posts</h1>
<table id='posts-table'>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <%= sync partial: 'index', collection: posts %>
    <%= sync_new partial: 'index', resource: Post.new %>
  </tbody>
</table>
<br>